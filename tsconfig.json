{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "target": "ES2015",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "typeRoots": ["server/typings", "node_modules/@types"],
    "allowJs": true,
    "outDir": "dist"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/**/*"]
}
